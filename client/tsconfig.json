{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "Node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "rootDir": ".",
    "baseUrl": "src",
    "paths": {
      "assets/*": [
        "assets/*"
      ],
      "state/*": [
        "state/*"
      ],
      "objects/*": [
        "objects/*"
      ]
    },
  },
  "include": [
    "src",
    "src/svg.d.ts"
  ],
}
