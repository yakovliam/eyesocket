{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "assets/*": [
        "assets/*"
      ],
      "state/*": [
        "state/*"
      ],
      "objects/*": [
        "objects/*"
      ]
    },
    "outDir": "./dist",
    "rootDir": "."
  },
  "include": [
    "src",
    "src/svg.d.ts"
  ],
  "references": [
    {
      "path": "../common"
    }
  ]
}
